  "description": "webpack h5 demo",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development API_MODE=pre webpack-dev-server --hot --progress --config build/webpack.dev.js",
    "dev": "NODE_ENV=development API_MODE=pro webpack-dev-server --hot --progress --config build/webpack.dev.js",
    "build:pre": "NODE_ENV=production API_MODE=pre webpack --progress --config build/webpack.prod.js",
    "build": "NODE_ENV=production API_MODE=pro webpack --progress --config build/webpack.prod.js",
    "build:stats": "NODE_ENV=production API_MODE=pro NODE_STATS=true webpack --progress --config build/webpack.prod.js --profile --json > stats.json"
  },
